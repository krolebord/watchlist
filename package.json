{
  "name": "watchlist",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "wrangler dev",
    "dev:app": "vite",
    "cf-typegen": "wrangler types",
    "format": "biome check --write",
    "build": "tsc -b && vite build",
    "deploy": "pnpm run build && wrangler deploy",
    "db:main:generate": "drizzle-kit generate --config=\"worker/main/db/drizzle.config.ts\"",
    "db:main:migrate:prod": "wrangler d1 migrations apply watchlist-main --remote",
    "db:main:migrate:local": "wrangler d1 migrations apply watchlist-main --local",
    "db:main:studio:prod": "drizzle-kit studio --config=\"worker/main/db/drizzle.config.ts\"",
    "db:main:studio:local": "cross-env LOCAL=true drizzle-kit studio --config=\"worker/main/db/drizzle.config.ts\"",
    "db:main:push:local": "cross-env LOCAL=true drizzle-kit push --config=\"worker/main/db/drizzle.config.ts\""
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@cloudflare/workers-types": "^4.20250124.3",
    "@tanstack/router-devtools": "^1.97.14",
    "@tanstack/router-plugin": "^1.97.14",
    "@types/node": "^22.10.10",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "better-sqlite3": "^11.8.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "drizzle-kit": "^0.30.2",
    "postcss": "^8.5.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3",
    "vite": "^6.0.11",
    "wrangler": "^3.105.1"
  },
  "dependencies": {
    "@formkit/auto-animate": "^0.8.2",
    "@radix-ui/react-dropdown-menu": "^2.1.5",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-popover": "^1.1.5",
    "@radix-ui/react-select": "^2.1.5",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-toggle": "^1.1.1",
    "@radix-ui/react-tooltip": "^1.1.7",
    "@tanstack/react-query": "^5.64.2",
    "@tanstack/react-router": "^1.97.14",
    "@trpc/client": "11.0.0-rc.682",
    "@trpc/react-query": "11.0.0-rc.682",
    "@trpc/server": "11.0.0-rc.682",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.38.4",
    "lucide-react": "^0.469.0",
    "motion": "^11.18.2",
    "partyserver": "^0.0.59",
    "partysocket": "^1.0.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "superjson": "^2.2.2",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "tmdb-ts": "^2.0.1",
    "zod": "^3.24.1"
  }
}
