{
  "name": "watchlist",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "dev:app": "vite",
    "cf-typegen": "wrangler types",
    "format": "biome check --write",
    "build": "tsc -b && vite build",
    "deploy": "pnpm run build && wrangler deploy",
    "db:generate": "drizzle-kit generate --config=\"worker/main/db/drizzle.config.ts\"",
    "db:migrate:prod": "wrangler d1 migrations apply watchlist-main --remote",
    "db:migrate:local": "wrangler d1 migrations apply watchlist-main --local",
    "db:studio:prod": "drizzle-kit studio --config=\"worker/main/db/drizzle.config.ts\"",
    "db:studio:local": "cross-env LOCAL=true drizzle-kit studio --config=\"worker/main/db/drizzle.config.ts\"",
    "db:push:local": "cross-env LOCAL=true drizzle-kit push --config=\"worker/main/db/drizzle.config.ts\"",
    "email:preview": "email dev --dir worker/emails"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@cloudflare/workers-types": "^4.20250510.0",
    "@tanstack/router-devtools": "^1.120.3",
    "@tanstack/router-plugin": "^1.120.3",
    "@types/node": "^22.15.17",
    "@types/react": "^19.1.3",
    "@types/react-dom": "^19.1.4",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "better-sqlite3": "^11.10.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.5.0",
    "drizzle-kit": "^0.30.6",
    "postcss": "^8.5.3",
    "react-email": "^3.0.7",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-pwa": "^0.21.2",
    "wrangler": "^3.114.8"
  },
  "dependencies": {
    "@formkit/auto-animate": "^0.8.2",
    "@hookform/resolvers": "^3.10.0",
    "@oslojs/otp": "^1.1.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "@radix-ui/react-avatar": "^1.1.9",
    "@radix-ui/react-context-menu": "^2.2.14",
    "@radix-ui/react-dialog": "^1.1.13",
    "@radix-ui/react-dropdown-menu": "^2.1.14",
    "@radix-ui/react-label": "^2.1.6",
    "@radix-ui/react-menubar": "^1.1.14",
    "@radix-ui/react-popover": "^1.1.13",
    "@radix-ui/react-scroll-area": "^1.2.8",
    "@radix-ui/react-select": "^2.2.4",
    "@radix-ui/react-separator": "^1.1.6",
    "@radix-ui/react-slider": "^1.3.4",
    "@radix-ui/react-slot": "^1.2.2",
    "@radix-ui/react-toggle": "^1.1.8",
    "@radix-ui/react-tooltip": "^1.2.6",
    "@react-email/components": "^0.0.33",
    "@react-email/render": "^1.1.1",
    "@tanstack/react-query": "^5.76.0",
    "@tanstack/react-router": "^1.120.3",
    "@tanstack/zod-adapter": "^1.120.3",
    "@trpc/client": "11.0.0",
    "@trpc/react-query": "11.0.0",
    "@trpc/server": "11.0.0",
    "ai": "^4.3.15",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cookie": "^1.0.2",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.40.1",
    "lucide-react": "^0.476.0",
    "motion": "^12.11.0",
    "partyserver": "^0.0.64",
    "partysocket": "^1.1.4",
    "react": "^19.1.0",
    "react-day-picker": "8.10.1",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.56.3",
    "remeda": "^2.21.4",
    "resend": "^4.5.1",
    "spin-delay": "^2.0.1",
    "superjson": "^2.2.2",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "tmdb-ts": "^2.0.1",
    "trpc-to-openapi": "^2.3.0",
    "vaul": "^1.1.2",
    "workers-ai-provider": "^0.3.2",
    "zod": "^3.24.4",
    "zustand": "^5.0.4"
  }
}
