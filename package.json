{
  "name": "watchlist",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "dev:app": "vite",
    "cf-typegen": "wrangler types",
    "format": "biome check --write",
    "build": "tsc -b && vite build",
    "deploy": "pnpm run build && wrangler deploy",
    "db:generate": "drizzle-kit generate --config=\"worker/main/db/drizzle.config.ts\"",
    "db:migrate:prod": "wrangler d1 migrations apply watchlist-main --remote",
    "db:migrate:local": "wrangler d1 migrations apply watchlist-main --local",
    "db:studio:prod": "drizzle-kit studio --config=\"worker/main/db/drizzle.config.ts\"",
    "db:studio:local": "cross-env LOCAL=true drizzle-kit studio --config=\"worker/main/db/drizzle.config.ts\"",
    "db:push:local": "cross-env LOCAL=true drizzle-kit push --config=\"worker/main/db/drizzle.config.ts\"",
    "email:preview": "email dev --dir worker/emails"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@cloudflare/workers-types": "^4.20250224.0",
    "@tanstack/router-devtools": "^1.112.0",
    "@tanstack/router-plugin": "^1.112.0",
    "@types/node": "^22.13.5",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "better-sqlite3": "^11.8.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "drizzle-kit": "^0.30.5",
    "postcss": "^8.5.3",
    "react-email": "^3.0.7",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3",
    "vite": "^6.2.0",
    "vite-plugin-pwa": "^0.21.1",
    "wrangler": "^3.111.0"
  },
  "dependencies": {
    "@formkit/auto-animate": "^0.8.2",
    "@hookform/resolvers": "^3.10.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-context-menu": "^2.2.6",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-menubar": "^1.1.6",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slider": "^1.2.3",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-toggle": "^1.1.2",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@react-email/components": "^0.0.33",
    "@react-email/render": "^1.0.5",
    "@tanstack/react-query": "^5.66.10",
    "@tanstack/react-router": "^1.112.0",
    "@tanstack/zod-adapter": "^1.112.0",
    "@trpc/client": "11.0.0-rc.682",
    "@trpc/react-query": "11.0.0-rc.682",
    "@trpc/server": "11.0.0-rc.682",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cookie": "^1.0.2",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.40.0",
    "lucide-react": "^0.476.0",
    "motion": "^12.4.7",
    "partyserver": "^0.0.64",
    "partysocket": "^1.0.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "remeda": "^2.21.0",
    "resend": "^4.1.2",
    "spin-delay": "^2.0.1",
    "superjson": "^2.2.2",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "tmdb-ts": "^2.0.1",
    "vaul": "^1.1.2",
    "zod": "^3.24.2",
    "zustand": "^5.0.3"
  }
}
